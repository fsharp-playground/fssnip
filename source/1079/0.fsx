let carray_to_bytes (xs : string) = 
  xs.Split '{'
  |> Seq.skip 1
  |> Seq.head
  |> fun s -> 
     let newstr = s.Replace(" ", "").Replace("\n","")
     newstr.TrimEnd [|'}';';'|]  
     |> fun ns -> ns.Split ','         
     |> Seq.map (int >> byte)
     |> Seq.toArray

